.c-Telephone-Number {
    display: inline-block;
    font-family: var(--font-sans-en);;
}

.c-Telephone-Number_Link {
    font-size: 34px;
    display: block;
    line-height: var(--leading-x-tight);

    &::before {
        content: url(../images/tel.svg);
        display: inline-block;
        margin-right: 8px;
    }
}

.c-Telephone-Number_Date {
    text-align: right;
    font-size: 14px;
}