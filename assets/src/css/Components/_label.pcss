.c-category-Label {
    border: 1px solid var(--color-label);
    background-color: var(--color-white);
    color: var(--color-label);
    text-align: center;
    display: inline-block;
    padding: 5px 10px;
    font-size: 12px;
    font-weight: bold;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    line-height: var(--leading-none);
}