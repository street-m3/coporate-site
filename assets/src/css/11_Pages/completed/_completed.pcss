[data-placement="completed"] {
    & p:not([class]) {
        margin-bottom: 1.5em;
        line-height: $leading-x-loose;
        letter-spacing: $tracking-wide;
    }
}

.p-completed-underComponent {
    margin-top: clamp(50px, 8.680vw, 70px);
    text-align: center;

    & > * {
        padding: 16px;
        color: var(--color-main);
        font-size: 14px;
    }
}